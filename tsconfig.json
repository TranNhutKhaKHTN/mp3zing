{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@containers/*": [
        "containers/*"
      ],
      "@redux/*": [
        "redux/*"
      ],
      "@public/*": [
        "public/*"
      ]
      // "@atoms/*": [
      //   "containers/../atoms/*"
      // ],
      // "@modlecules/*": [
      //   "containers/modlecules/*"
      // ],
      // "@organisms/*": [
      //   "containers/organisms/*"
      // ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "containers/home/organisms/hottopic/index.js",
    "redux/reducers/homeReducer/index.js",
    "redux/reducers/playmusicReducer/index.js"
  ],
  "exclude": [
    "node_modules"
  ]
}